  },
  "homepage": "https://github.com/anand-kamble/mcp-instagram#readme",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "instagram-private-api": "^1.46.1",
    "re2": "^1.22.3"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
